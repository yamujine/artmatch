{% extends "base.twig" %}

{% block contents %}
	<div class="ui segment">
		<img class="ui small left floated image" src="data:image/jpeg;base64,">
		<p>{{ user.name }} ({{ user.email }})</p>
		<p>받은 picks: {{ given_pick_count }}</p>
		<p><a class="ui button blue">프로필 사진 변경하기</a><a class="ui button blue">패스워드 변경하기</a></p>
	</div>
	
	<div class="ui top attached tabular menu">
		<div class="item {% if type == 'artworks' %}active{% endif %}">
			<a href="?type=artworks">작품 picks</a>
		</div>
		<div class="item {% if type == 'places' %}active{% endif %}">
			<a href="?type=places">장소 picks</a>
		</div>
	</div>
	
	<div class="ui bottom attached active tab segment">
		<div class="ui small images">
			{% for pick in my_picks %}
				<a href="/{{ type }}/{% if type == 'artworks' %}{{ pick.artwork_id }}{% elseif type == 'places' %}{{ pick.place_id }}{% endif %}" class="ui medium image">
					<img src="data:image/jpeg;base64,">
					{% if type == 'artworks' %}
						{{ pick.artwork.title }}
					{% elseif type == 'places' %}
						{{ pick.place.name }}
					{% endif %}
				</a>
			{% endfor %}
		</div>
	</div>
{% endblock %}
