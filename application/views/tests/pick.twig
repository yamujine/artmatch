{% extends "base.twig" %}

{% block contents %}

    <form method="POST" enctype="multipart/form-data" action="/api/pick">
        <div class="form-group">
            <label>type</label>
            <input class="form-control" type="text" name="type" placeholder="" value="artwork">
        </div>
        <div class="form-group">
            <label>type_id</label>
            <input class="form-control" type="text" name="type_id" placeholder="" value="1">
        </div>

        <button class="btn btn-default" type="submit">Submit</button>
    </form>

{% endblock %}
