<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>places</title>
</head>
<body>
places
<table>
    <thead>
    <th>id</th>
    <th>user_id</th>
    <th>status</th>
    <th>name</th>
    <th>description</th>
    <th>image</th>
    <th>address</th>
    <th>use_comment</th>
    <th>tags</th>
    <th>upload_date</th>
    </thead>
    <tbody>
{% for place in places %}
    <tr>
        <td>{{ place.id }}</td>
        <td>{{ place.status }}</td>
        <td>{{ place.status }}</td>
        <td>{{ place.name }}<br>
        <td>{{ place.description }}<br>
        <td>{{ place.image }}<br>
        <td>{{ place.address }}<br>
        <td>{{ place.use_comment }}<br>
        <td>{{ place.tags }}<br>
        <td>{{ place.upload_date }}<br>
    </tr>
{% endfor %}
    </tbody>
</table>
</body>
</html>